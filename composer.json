{
    "name": "catamc90/font-awesome-pro",
    "description": "Font Awesome Pro integration for Symfony projects",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "YourNamespace\\FontAwesomePro\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/catamc90/font-awesome-pro"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "YourNamespace\\FontAwesomePro\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
